<mvc:View
    controllerName="com.car.assignment6.controller.Question3"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
>
    <Shell id="shell">
        <App id="app">
            <pages>
                <Page id="page" title="{i18n>title}">
                    <content>
                    <IconTabBar id="_IDGenIconTabBar1" class="dtb">
                    <items>
                      
               <IconTabFilter id="_IDGenIconTabFilter8" icon="sap-icon://account">
			</IconTabFilter>
             <IconTabFilter id="_IDGenIconTabFilter7" icon="sap-icon://home">
			</IconTabFilter>
            <IconTabFilter id="_IDGenIconTabFilter5" icon="sap-icon://search">
			</IconTabFilter>
            
                    </items>
                    </IconTabBar>
                    <Carousel id="_IDGenCarousel1" width="100%" height="35%" loop="true">
                   
		<Image id="_IDGenImage2" src="https://imgd.aeplcdn.com/1056x594/n/cw/ec/27074/civic-exterior-right-front-three-quarter-148156.jpeg" alt="honda1" />
		<Image id="_IDGenImage3" src="https://bsmedia.business-standard.com/_media/bs/img/article/2020-12/23/full/1608746810-3598.jpg" alt="honda2" />
		<Image id="_IDGenImage4" src="https://imgd.aeplcdn.com/664x374/n/cw/ec/40535/all-new-city-exterior-right-front-three-quarter.jpeg" />
                    </Carousel>
                    
                    <HBox id="hbox1" class="hbox1" justifyContent="Start">
                    <Panel id="panel1" class="pnl1 pnlh" >
                    <Link id="btn2" text="View Service Status"></Link>
                    </Panel>
                    </HBox>

                    <l:VerticalLayout id="_IDGenVerticalLayout1" class="sapUiContentPadding" width="100%">
		<Text id="_IDGenText1" text="Enter your feedback here:-" class="sapUiTinyMarginTop"/>
		<TextArea id="textAreaWithBinding2" value="{/value}" showExceededText="true" maxLength="40" width="100%" valueState="{= ${/value}.length > 40 ? 'Warning' : 'None' }" valueLiveUpdate="false" liveChange="handleLiveChange" />
	<Button id="btn4" text="Submit your Feedback"></Button>
    </l:VerticalLayout>

    <Input id="ip" placeholder="{data>/details/name}" ></Input>
    </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>
